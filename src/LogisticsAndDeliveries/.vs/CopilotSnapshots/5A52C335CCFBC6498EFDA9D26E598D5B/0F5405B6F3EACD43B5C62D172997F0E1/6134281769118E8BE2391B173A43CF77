using System;
using System.Collections.Generic;

namespace LogisticsAndDeliveries.Infrastructure.Persistence.PersistenceModel.EFCoreEntities
{
    internal class DeliveryPersistenceModel
    {
        public Guid DeliveryId { get; set; }
        public string Name { get; set; }
        public ICollection<PackageAssignmentPersistenceModel> PackageAssignments { get; set; }
        public ICollection<DeliveryLocationPersistenceModel> DeliveryLocations { get; set; }
        public ICollection<DeliveryRoutePersistenceModel> DeliveryRoutes { get; set; }
    }
}
